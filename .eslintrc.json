{
  "env": {
    "browser": true,
    "jasmine": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "jQuery": false,
    "$": false,
    "_": false
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "dot-notation": [
      "error"
    ],
    "brace-style": [
      "error",
      "1tbs"
    ],
    "comma-style": [
      "error",
      "last"
    ],
    "block-scoped-var": [
      "error"
    ],
    "curly": [
      "error"
    ],
    "default-case": [
      "error"
    ],
    "dot-notation": [
      "error"
    ],
    "eqeqeq": [
      "error",
      "always"
    ],
    "no-eval": [
      "error"
    ],
    "no-fallthrough": [
      "error"
    ],
    "no-implied-eval": [
      "error"
    ],
    "no-multi-spaces": [
      "error",
      {
        "exceptions": {
          "Property": true,
          "VariableDeclarator": true
        }
      }
    ],
    "no-multi-str": [
      "error"
    ],
    "no-new": [
      "error"
    ],
    "no-global-assign": [
      "error"
    ],
    "yoda": [
      "error",
      "never",
      {
        "exceptRange": true
      }
    ],
    "camelcase": [
      "error"
    ],
    "strict": [
      "error",
      "global"
    ],
    "key-spacing": [
      "error",
      {
        "beforeColon": false,
        "afterColon": true,
        "align": "colon"
      }
    ],
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1,
        "MemberExpression": 1,
        "VariableDeclarator": {
          "var": 2,
          "let": 2,
          "const": 3
        }
      }
    ]
  }
}
